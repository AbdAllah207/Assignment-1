   "source": [
    "def tanh(x):\n",
    "    return (2 / (1 + (2.71828 ** (-2 * x)))) - 1  \n",
    "\n",
    "w1 = 0.4 \n",
    "w2 = -0.1  \n",
    "\n",
    "b1 = 0.5\n",
    "b2 = 0.7\n",
    "\n",
    "x1 = 0.8\n",
    "x2 = -0.2\n",
    "\n",
    "h1 = tanh((w1 * x1) + (w2 * x2) + b1)\n",
    "output = tanh(h1 + b2)\n",
    "\n",
    "print(\"out H1:\", h1)\n",
    "print(\"Output:\", output)"
   ]
